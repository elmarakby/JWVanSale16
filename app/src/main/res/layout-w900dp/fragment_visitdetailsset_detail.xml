            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:keyText="Customername"
            app:valueText="@{visitDetails.getOptionalValue(VisitDetails.customername).toString()}" />

        <com.sap.cloud.mobile.fiori.misc.KeyValueCell
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:keyText="Countrycode"
            app:valueText="@{visitDetails.getOptionalValue(VisitDetails.countrycode).toString()}" />

        <com.sap.cloud.mobile.fiori.misc.KeyValueCell
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:keyText="Street"
            app:valueText="@{visitDetails.getOptionalValue(VisitDetails.street).toString()}" />

        <com.sap.cloud.mobile.fiori.misc.KeyValueCell
            android:layout_width="match_parent"
