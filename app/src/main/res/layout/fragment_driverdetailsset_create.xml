                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/fvarid"
                        android:tag="Fvar"
                        android:maxLength="@{30}"
                        app:key="Fvar"
                        app:value="@={driverDetails.fvar}">
                </com.sap.cloud.mobile.fiori.formcell.SimplePropertyFormCell>
                <com.sap.cloud.mobile.fiori.formcell.SimplePropertyFormCell
                        xmlns:android="http://schemas.android.com/apk/res/android"
                        xmlns:app="http://schemas.android.com/apk/res-auto"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/fvar2id"
                        android:tag="Fvar2"
                        android:maxLength="@{30}"
                        app:key="Fvar2"
                        app:value="@={driverDetails.fvar2}">
                </com.sap.cloud.mobile.fiori.formcell.SimplePropertyFormCell>
                <com.sap.cloud.mobile.fiori.formcell.SimplePropertyFormCell
                        xmlns:android="http://schemas.android.com/apk/res/android"
                        xmlns:app="http://schemas.android.com/apk/res-auto"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/fvar3id"
                        android:tag="Fvar3"
                        android:maxLength="@{30}"
                        app:key="Fvar3"
                        app:value="@={driverDetails.fvar3}">
                </com.sap.cloud.mobile.fiori.formcell.SimplePropertyFormCell>
                <com.sap.cloud.mobile.fiori.formcell.SimplePropertyFormCell
                        xmlns:android="http://schemas.android.com/apk/res/android"
                        xmlns:app="http://schemas.android.com/apk/res-auto"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/fvar4id"
                        android:tag="Fvar4"
                        android:maxLength="@{30}"
                        app:key="Fvar4"
                        app:value="@={driverDetails.fvar4}">
                </com.sap.cloud.mobile.fiori.formcell.SimplePropertyFormCell>
                <com.sap.cloud.mobile.fiori.formcell.SimplePropertyFormCell
                        xmlns:android="http://schemas.android.com/apk/res/android"
                        xmlns:app="http://schemas.android.com/apk/res-auto"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/fvar5id"
                        android:tag="Fvar5"
                        android:maxLength="@{30}"
                        app:key="Fvar5"
                        app:value="@={driverDetails.fvar5}">
                </com.sap.cloud.mobile.fiori.formcell.SimplePropertyFormCell>
                <com.sap.cloud.mobile.fiori.formcell.SimplePropertyFormCell
                        xmlns:android="http://schemas.android.com/apk/res/android"
                        xmlns:app="http://schemas.android.com/apk/res-auto"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/fvar6id"
                        android:tag="Fvar6"
                        android:maxLength="@{30}"
                        app:key="Fvar6"
                        app:value="@={driverDetails.fvar6}">
                </com.sap.cloud.mobile.fiori.formcell.SimplePropertyFormCell>
                <com.sap.cloud.mobile.fiori.formcell.SimplePropertyFormCell
                        xmlns:android="http://schemas.android.com/apk/res/android"
                        xmlns:app="http://schemas.android.com/apk/res-auto"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/fvar7id"
                        android:tag="Fvar7"
                        android:maxLength="@{30}"
                        app:key="Fvar7"
                        app:value="@={driverDetails.fvar7}">
                </com.sap.cloud.mobile.fiori.formcell.SimplePropertyFormCell>
                <com.sap.cloud.mobile.fiori.formcell.SimplePropertyFormCell
                        xmlns:android="http://schemas.android.com/apk/res/android"
                        xmlns:app="http://schemas.android.com/apk/res-auto"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/fvar8id"
                        android:tag="Fvar8"
                        android:maxLength="@{30}"
                        app:key="Fvar8"
                        app:value="@={driverDetails.fvar8}">
                </com.sap.cloud.mobile.fiori.formcell.SimplePropertyFormCell>
                <com.sap.cloud.mobile.fiori.formcell.SimplePropertyFormCell
                        xmlns:android="http://schemas.android.com/apk/res/android"
                        xmlns:app="http://schemas.android.com/apk/res-auto"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/fvar9id"
                        android:tag="Fvar9"
                        android:maxLength="@{30}"
                        app:key="Fvar9"
                        app:value="@={driverDetails.fvar9}">
                </com.sap.cloud.mobile.fiori.formcell.SimplePropertyFormCell>
                <com.sap.cloud.mobile.fiori.formcell.SimplePropertyFormCell
                        xmlns:android="http://schemas.android.com/apk/res/android"
                        xmlns:app="http://schemas.android.com/apk/res-auto"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/fvar10id"
                        android:tag="Fvar10"
                        android:maxLength="@{30}"
                        app:key="Fvar10"
                        app:value="@={driverDetails.fvar10}">
                </com.sap.cloud.mobile.fiori.formcell.SimplePropertyFormCell>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</layout>