            android:layout_height="wrap_content"
            app:keyText="Fvar3"
            app:valueText="@{visitDetails.getOptionalValue(VisitDetails.fvar3).toString()}" />

        <com.sap.cloud.mobile.fiori.misc.KeyValueCell
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:keyText="Fvar4"
            app:valueText="@{visitDetails.getOptionalValue(VisitDetails.fvar4).toString()}" />

