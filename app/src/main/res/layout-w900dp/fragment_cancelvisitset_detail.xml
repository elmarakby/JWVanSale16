<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        tools:context=".mdui.cancelvisitset.CancelVisitSetActivity">
    <data>
        <import type = "com.sap.cloud.android.odata.zgw_vansale_srv_entities.CancelVisit" />
        <variable name="cancelVisit" type="CancelVisit"/>
        <variable name="handler" type="com.labs.jwvansale16.mdui.cancelvisitset.CancelVisitSetDetailFragment" />
    </data>

