<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        tools:context=".mdui.visitdetailsset.VisitDetailsSetActivity">
    <data>
        <import type = "com.sap.cloud.android.odata.zgw_vansale_srv_entities.VisitDetails" />
        <variable name="visitDetails" type="VisitDetails"/>
        <variable name="handler" type="com.labs.jwvansale16.mdui.visitdetailsset.VisitDetailsSetDetailFragment" />
    </data>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <com.sap.cloud.mobile.fiori.misc.KeyValueCell
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:keyText="Userid"
            app:valueText="@{visitDetails.getOptionalValue(VisitDetails.userid).toString()}" />

        <com.sap.cloud.mobile.fiori.misc.KeyValueCell
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:keyText="Driverid"
            app:valueText="@{visitDetails.getOptionalValue(VisitDetails.driverid).toString()}" />

        <com.sap.cloud.mobile.fiori.misc.KeyValueCell
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:keyText="Visitid"
            app:valueText="@{visitDetails.getOptionalValue(VisitDetails.visitid).toString()}" />

        <com.sap.cloud.mobile.fiori.misc.KeyValueCell
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:keyText="Visititemno"
            app:valueText="@{visitDetails.getOptionalValue(VisitDetails.visititemno).toString()}" />

        <com.sap.cloud.mobile.fiori.misc.KeyValueCell
