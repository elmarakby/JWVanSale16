<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        tools:context=".mdui.collectionsset.CollectionsSetActivity">
    <data>
        <import type = "com.sap.cloud.android.odata.zgw_vansale_srv_entities.Collections" />
        <variable name="collections" type="Collections"/>
        <variable name="handler" type="com.labs.jwvansale16.mdui.collectionsset.CollectionsSetDetailFragment" />
    </data>

    <LinearLayout
