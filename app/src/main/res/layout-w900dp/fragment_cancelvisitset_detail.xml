            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:keyText="Driverid"
            app:valueText="@{cancelVisit.getOptionalValue(CancelVisit.driverid).toString()}" />

        <com.sap.cloud.mobile.fiori.misc.KeyValueCell
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:keyText="Visitid"
            app:valueText="@{cancelVisit.getOptionalValue(CancelVisit.visitid).toString()}" />

